{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len":"off",
        "func-names":"off",
        "prefer-arrow-callback":"off",
        "no-console": "off",
        "no-unused-vars": "off",
        "radix":"off"
    }
}
